<h3>Create Employee</h3>
<%= form_for @employee do |f| %>
First Name:<%= f.text_field :first_name %>
Last Name:<%= f.text_field :last_name %>
Alias:<%= f.text_field :alias %>
Title:<%= f.text_field :title %>
Office:<%= f.text_field :office %>
Dog:<%= f.collection_select :dog_id, Dog.all, :id, :name, prompt: true %>
<%= f.submit %>
<% if @employee.errors.any? %>
  <div id="error_explanation">
    <h2><%= pluralize(@employee.errors.count, "error") %> Please check your title and alias for duplicates!</h2>
<% end %>
<% end %>